{% extends 'admin/base.html.twig' %}


{% block content %}
    <!-- Main content -->
    <section class="content container-fluid">

        <div class="box-body table-responsive no-padding">
            <table class="table table-hover">
                <tr>
                    <th>Product name</th>

                </tr>
                {% for product in products %}
                    <tr>
                        <td>{{ product.name }}</td>

                    </tr>
                {% endfor %}

            </table>
        </div>


    </section>
    <!-- /.content -->

    <!-- /.content-wrapper -->

{% endblock %}