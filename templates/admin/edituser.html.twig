{% extends 'base.html.twig' %}
{% block content %}
    <!-- Main content -->
    <section class="content container-fluid">
        {% for message in app.flashes('success') %}
            <div class="row mt-3">
                <div class="col">
                    <div class="alert alert-success">
                        {{ message }}
                    </div>
                </div>
            </div>
        {% endfor %}

        <div class="box-body table-responsive no-padding">
            <table class="table table-hover">
                <tr>
                    <th>Fist Name</th>
                    <th>Last Name</th>
                    <th>Email</th>
                    <th>Delete</th>
                </tr>

            </table>
        </div>


    </section>
    <!-- /.content -->

    <!-- /.content-wrapper -->

{% endblock %}
