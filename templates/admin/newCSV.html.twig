{% extends 'admin/base.html.twig' %}

{% block content %}
    {% for message in app.flashes('success') %}
        <div class="row mt-3">
            <div class="col">
                <div class="alert alert-success">
                    {{ message }}
                </div>
            </div>
        </div>
    {% endfor %}

    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button  class="btn">{{ button_label|default('Upload') }}</button>
    {{ form_end(form) }}

{% endblock %}
